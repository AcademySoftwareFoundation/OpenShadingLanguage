#include "mx_funcs.h"

shader mx_crossproduct_TYPE_SUFFIX 
    [[ string help = "Perform a cross product of two vectors" ]]
  (
    vector in1 = 0,
    vector in2 = 0,
    output vector out = 0
  )
{
    out = cross(in1, in2);
}
