#include "mx_funcs.h"

shader mx_rgbtohsv
    [[ string help = "Transform a color from RGB to HSV" ]]
  (
    TYPE in = TYPE_ZERO
        [[ string help = "Input color" ]],
    output TYPE out = TYPE_ZERO
  )
{
    out = transformc ("rgb", "hsv", in);
}
