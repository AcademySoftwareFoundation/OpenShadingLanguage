shader
test_mix_v_vector_u_vector_v_float (output color Cout = 0)
{
    float  val = 1.0/(2*raytype("camera"));
    float val2 = val/2;
    float val3 = val/3;
    float val4 = val/5;
    
    vector v1 = vector(u, v, val2);
    vector v2 = vector(val2, 0.5, val);
    vector v3 = vector(v, val, u);
    vector v4 = vector(1.0, val2, val3);
              
    vector rv = mix(v1,v2,u);              
        
    if (int(P[0]*64)%2==0) {
        rv = mix(v3,v4,v);
    }
            
    Cout = rv;
        
}
