#include "mx_funcs.h"

shader mx_ceil_TYPE_SUFFIX
    [[ string help = TYPE_STR" Find the smallest integer less than or equal to the parameter." ]]
  (
    TYPE in = TYPE_DEFAULT_IN,
    output TYPE out = TYPE_DEFAULT_OUT
  )
{
    out = ceil(in); 
}
