#include "mx_funcs.h"

shader mx_determinant_TYPE_SUFFIX
    [[ string help = TYPE_STR" the determinant of matrix in" ]]
  (
    TYPE in = TYPE_DEFAULT_IN,
    output float out = 0
  )
{
    out = determinant(in);
}
