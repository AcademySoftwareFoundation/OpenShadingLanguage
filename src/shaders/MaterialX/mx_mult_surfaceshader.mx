#include "mx_types.h"

shader SHADER_NAME(mx_mult_surfaceshader)
    [[ string help = "multiply surfaceshader closures" ]]
  (
    closure color in1 = 0,
    TYPE in2 = 1,
    output closure color out = 0
  )
{
    out = in1 * in2; 
}
