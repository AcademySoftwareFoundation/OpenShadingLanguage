#include "mx_funcs.h"

shader mx_divide_TYPE_SUFFIX_OTHERTYPE_SUFFIX
    [[ string help = TYPE_STR" divide an incoming float/color/vector by in2" ]]
  (
    TYPE in1 = TYPE_DEFAULT_IN,
    OTHERTYPE in2 = OTHERTYPE_ONE,
    output TYPE out = TYPE_DEFAULT_OUT
  )
{
    out = in1 / in2;
}
