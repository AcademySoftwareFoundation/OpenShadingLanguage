struct testStruct{
   color c;
   int i;
   float f;
};


shader
test (
    testStruct input = {0, 0, 0},
    output testStruct out = input
)
{
    printf("testStruct: %g\n", input.c);
    printf("testStruct: %g\n", input);
    printf("testStruct: %d\n", input);
}
