#include "mx_funcs.h"

shader mx_max_TYPE_SUFFIX_OTHERTYPE_SUFFIX
    [[ string help = "Select the maximum among incoming values." ]]
  (
    TYPE in1 = TYPE_DEFAULT_IN,
    OTHERTYPE in2 = OTHERTYPE_DEFAULT_IN,
    output TYPE out = TYPE_DEFAULT_OUT
  )
{
    out = max(in1, in2);
}
