#include "mx_funcs.h"

shader mx_multiply_TYPE_SUFFIX
    [[ string help = TYPE_STR" multiply in1 by in2" ]]
  (
    TYPE in1 = TYPE_DEFAULT_IN,
    TYPE in2 = TYPE_ONE,
    output TYPE out = TYPE_DEFAULT_OUT
  )
{
    out = in1 * in2; 
}
