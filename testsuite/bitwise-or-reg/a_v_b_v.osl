shader a_v_b_v(output vector cout = 0, output vector mcout = 0) 
{
    int a = int(P[0])*10;
    int b = int(P[1])*3;
    
    int c = a|b;
    cout = vector(tan(u), tan(P[0]/c), v*u);
    vector mcout_ = cout;
    
    if(int(P[0]*32)%2)
    {
        a = int(u)*2;
        b = int(P[1])*4;
        c = a|b;
        mcout_ = vector(tan(v/c), tan(P[1]), u/v);
    }
    
    mcout = mcout_;
}
